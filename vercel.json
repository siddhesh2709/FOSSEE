{
  "version": 2,
  "name": "chemical-equipment-visualizer",
  "builds": [
    {
      "src": "frontend-web/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend-web/$1"
    }
  ]
}
